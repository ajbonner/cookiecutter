{
  "name": "Cookie Cutter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Zaps page view counting cookies from the telegraph.co.uk website",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" 
  }, 
  "background": {
    "scripts": [ "background.js" ]
  },
  "permissions": [
    "cookies", "tabs", "<all_urls>"
  ]
}
